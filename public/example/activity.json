{
    "ROOT": {
        "nodes": [
            {
                "type": "Context",
                "props": {
                    "context": {
                        "$type": "$data",
                        "path": "$store.apis.custodian.activity.getByPk[{\"$type\":\"$expression\",\"expression\":\"+data('$route.params.id')\"}]"
                    }
                },
                "nodes": [
                    {
                        "type": "GridCell",
                        "nodes": [
                            {
                                "type": "GridCell",
                                "props": {
                                    "children": {
                                        "$type": "$data",
                                        "path": "$route.params.id"
                                    }
                                }
                            },
                            {
                                "type": "GridCell",
                                "props": {
                                    "children": {
                                        "$type": "$data",
                                        "path": "$context.name"
                                    }
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
}
